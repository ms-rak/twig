</li>
{% endmacro %}
{% macro bestbrand(brand ,loopIndex) %}
<div class="box--1-2 brand" style="background-color: {{brand.background}}">
  <a href="{{brand.url}}?icid={{ marketplace }}_top_brand_{{loopIndex}}"><img src="{{brand.imgUrl}}"></a>
</div>
{% endmacro %}
{% macro topbanner(banner) %}
<div data-ietype="image" data-imgsize="800x50" class="top-banner-wrap ieditable" style="background-image: url({{banner.imgUrl}})">
  <a href="{{banner.url}}" class="full" target="_blank"></a>
</div>
{% endmacro %}
{% macro carouselbanner(banner, loopIndex) %}
<article >
   <a href="{{banner.url}}?icid={{ marketplace }}_top_topcarousel_pos{{ loopIndex }}_{{banner.trackingName}}" >
      <div class="product--image">
         <div class="product--image-wp">
            <img src="{{banner.imgUrl}}">
         </div>
      </div>
   </a>
</article>
{% endmacro %}

{% macro shopoftheweek(shop, loopIndex) %}
<div class="box--1-2 shop">
   <a href="{{shop.url}}?icid={{ marketplace }}_top_shopoftheweek_pos{{ loopIndex }}_{{shop.name | raw}}" class="full"></a>
   <img src="{{shop.imgUrl}}">
   <p>
      <span class="headline">{{shop.name | raw}}</span>
      {{shop.descr | raw}}
      <span class="goto"><span class="icon"></span>ZUM SHOP</span>
   </p>
</div>
{% endmacro %}

{% macro sky(banner) %}
  <a href="{{banner.url}}?icid={{ marketplace }}_susatop_skyscraper_{{banner.trackingName | raw}}" target="_blank">
    <img src="{{banner.imgUrl}}" alt="">
  </a>
{% endmacro %}

{% macro tab(tab) %}
  <span class="tab"> 
    <a href="{{tab.url}}" title="NAME">{{tab.name}}</a>
  </span>
{% endmacro %}

{% macro floorbanner(banner) %}
<a href="{{banner.url}}?icid={{ marketplace }}_top_floorbanner_{{banner.trackingName | raw}}">
  <img src="{{banner.imgUrl}}">
</a>
{% endmacro %}

{% macro floorProduct(product, trackingName, loopIndex, slideWidth)%}
{% set details = product.details %}
{% set image = details.images | first %}
  <article class="{{slideWidth}}">
   <div class="CR--product product">
      <a href="{{ details.url }}?icid={{ marketplace }}_top_{{ trackingName }}_pos{{ loopIndex }}_{{ details.id }}" title="{{ details.name | raw }}">
